{
  "name": "tth-frontend",
  "version": "1.0.0",
  "main": "./src/index.ts",
  "type": "module",
  "author": "Lucas Farias",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trackthehabit/tth-frontend.git"
  },
  "scripts": {
    "start": "NODE_ENV=production node ./dist/src/index.js",
    "dev": "NODE_ENV=development nodemon ./src/index.ts",
    "build": "npm run build:react && npm run build:server",
    "build:server": "tsc -p tsconfig.server.json",
    "build:react": "npm run build:react:client && npm run build:react:ssr",
    "build:react:ssr": "vite build --config ./vite.config.ssr.ts",
    "build:react:client": "vite build --config ./vite.config.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{js,jsx,ts,tsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --quiet",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@headlessui/react": "^1.7.14",
    "@reduxjs/toolkit": "^1.9.1",
    "@tanstack/react-query": "^4.22.0",
    "axios": "^1.2.3",
    "classnames": "^2.3.2",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.9",
    "react-redux": "^8.0.5",
    "react-router-dom": "^6.6.2",
    "react-tailwindcss-datepicker": "^1.6.1",
    "serialize-javascript": "^6.0.1",
    "serve-static": "^1.15.0"
  },
  "devDependencies": {
    "@swc/core": "^1.3.57",
    "@swc/helpers": "^0.5.1",
    "@testing-library/react": "^13.4.0",
    "@trivago/prettier-plugin-sort-imports": "^4.0.0",
    "@types/compression": "^1.7.2",
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "@types/serialize-javascript": "^5.0.2",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "@vitejs/plugin-react": "^3.0.1",
    "@vitejs/plugin-react-swc": "^3.2.0",
    "@vitest/coverage-c8": "^0.27.1",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-import-resolver-typescript": "^3.5.3",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.7.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "happy-dom": "^8.1.3",
    "nodemon": "^2.0.22",
    "postcss": "^8.4.21",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "regenerator-runtime": "^0.13.11",
    "rollup-plugin-analyzer": "^4.0.0",
    "tailwindcss": "^3.2.4",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.4",
    "vite": "^4.0.4",
    "vite-tsconfig-paths": "^4.0.3",
    "vitest": "^0.27.1",
    "vitest-fetch-mock": "^0.2.1"
  },
  "peerDependencies": {
    "dayjs": "^1.11.6",
    "react": "^18.2.0"
  }
}
